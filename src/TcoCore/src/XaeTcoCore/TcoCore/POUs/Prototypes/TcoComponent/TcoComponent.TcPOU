      </Implementation>
    </Method>
    <Method Name="UnregisterRestorer" Id="{478dbece-5fa5-0299-3030-b7e2be7fdfbb}">
      <Declaration><![CDATA[METHOD UnregisterRestorer : BOOL
VAR_INPUT
//	Restorer	:	REFERENCE TO ITcoObject;
	Restorer	:	ITcoObject;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _Restorer = Restorer THEN
	_Restorer := 0;
	UnregisterRestorer := TRUE;	
ELSE
	UnregisterRestorer := FALSE;	
END_IF
]]></ST>
      </Implementation>
    </Method>
