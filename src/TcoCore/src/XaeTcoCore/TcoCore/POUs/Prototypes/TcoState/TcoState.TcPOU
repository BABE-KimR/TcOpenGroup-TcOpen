        </Implementation>
      </Get>
    </Property>
    <Property Name="LastChangeState" Id="{de25d1df-a6dd-0e2e-177b-8473daeac0ce}">
      <Declaration><![CDATA[PROPERTY LastChangeState : INT]]></Declaration>
      <Get Name="Get" Id="{5d3b3d1c-7d34-0aa9-2f0d-d5bb49fbd5b1}">
