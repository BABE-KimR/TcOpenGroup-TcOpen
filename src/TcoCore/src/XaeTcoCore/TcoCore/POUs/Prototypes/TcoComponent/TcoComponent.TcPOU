    <Method Name="RegisterRestorer" Id="{e392551f-f765-0482-0bc8-4215d707559c}">
      <Declaration><![CDATA[METHOD RegisterRestorer : BOOL
VAR_INPUT
//	Restorer	:	REFERENCE TO ITcoObject;
	Restorer	:	ITcoObject;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[//IF _Restorer = 0 THEN
//	_Restorer REF= Restorer;
//	RegisterRestorer := TRUE;	
//ELSE
//	RegisterRestorer := FALSE;	
//END_IF

IF _Restorer = 0 THEN
	_Restorer := Restorer;
	RegisterRestorer := TRUE;	
ELSE
	RegisterRestorer := FALSE;	
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="Restore" Id="{d0c02f91-fc49-4476-8612-b6c1e2ed01ac}">
